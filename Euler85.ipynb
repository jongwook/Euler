   "source": [
    "from itertools import product\n",
    "\n",
    "def rectangles(x, y):\n",
    "    return sum((x + 1 - a) * (y + 1 - b) for a, b in product(range(1, x+1), range(1, y+1)))\n",
    "\n",
    "x, y = min(product(range(1, 100), repeat=2), key=lambda (x, y): abs(rectangles(x, y) - 2*10**6))\n",
    "\n",
    "print x*y\n",
    " "
   ]
  }
