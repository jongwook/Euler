    "from fractions import gcd\n",
    "\n",
    "ceiling = 12000\n",
    "\n",
    "print sum(1 for denom in range(4, ceiling + 1) for num in range(denom//3 + 1, denom//2 + 1) if gcd(num, denom) == 1)"
   ]
  }
